<Page
    x:Class="UWPgui.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:UWPgui"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <NavigationView x:Name="NavView">
            <NavigationView.MenuItems>
                <NavigationViewItemHeader Content="Real"/>
                <NavigationViewItem Icon="Street" Content="House" Tag="house"/>
                <NavigationViewItemHeader Content="Bullshit"/>
                <NavigationViewItem Icon="Contact2" Content="Something" Tag="something"/>
                <NavigationViewItem Icon="Camera" Content="Anything" Tag="anything"/>
                <NavigationViewItem Icon="Pictures" Content="Asd" Tag="asd"/>
            </NavigationView.MenuItems>
            
            <NavigationView.AutoSuggestBox>
                <AutoSuggestBox x:Name="ASB" QueryIcon="Find"/>
            </NavigationView.AutoSuggestBox>
        
        </NavigationView>

    </Grid>
</Page>
